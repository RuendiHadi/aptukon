if($nohd=="gmail.com"){
                $inserToUser = DB::TABLE('user')->insert($toUser);
                $insertToMasterMahasiswa = DB::TABLE('master_koordinator')->insert($toMasterMahasiswa);
                return redirect('/koor/home');
            }else if($nohd=="si.ukdw.ac.id"){
                $inserToUser = DB::TABLE('user')->insert($toUser);
                $insertToMasterMahasiswa = DB::TABLE('master_mahasiswa')->insert($toMasterMahasiswa);
                return redirect('/mahasiswa/home');
            }else if($nohd=="students.ukdw.ac.id"){
                $inserToUser = DB::TABLE('user')->insert($toUser);
               $insertToMasterMahasiswa = DB::TABLE('master_dosen')->insert($toMasterMahasiswa);
               return redirect('/dosen/home');
            }else{
                echo "Tidak ada";
            }
