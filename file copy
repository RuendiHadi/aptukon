$daftar_konsul = DB::table('master_konsultasi')
            ->join('master_mahasiswa', 'master_konsultasi.email', '=', 'master_mahasiswa.email')
            ->select('master_konsultasi.*', 'master_mahasiswa.*')
            ->get();
        return view('koor/lihat_jadwal_konsul',compact('daftar_konsul'));
